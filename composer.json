{
    "name": "nytris/intercooler",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nytris\\Intercooler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nytris\\Intercooler\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "nytris/nytris": "^0.1"
    },
    "require-dev": {
        "mockery/mockery": "1.6.11",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/phpunit": "^10.2",
        "symfony/process": "^6.0 || ^7.0"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
